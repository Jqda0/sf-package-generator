{
  "name": "sf-package-xml-generator",
  "displayName": "SF Package.xml Generator",
  "description": "Extension for generating Package.xml on the Salesforce Platform",
  "version": "1.1.0",
  "publisher": "jqda0",
  "repository": "https://github.com/Jqda0/sf-package-generator",
  "icon": "images/icon.png",
  "keywords": [
    "sf",
    "salesforce",
    "salesforce cli",
    "sf package xml",
    "salesforce package xml generator"
  ],
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Extension Packs",
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sfPackageGen.chooseMetadata",
        "title": "Choose Metadata Components",
        "category": "SF Package Generator"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint",
    "watch": "tsc -w -p ./",
    "test": "mocha",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "clipboardy": "^4.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@types/clipboardy": "^1.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20",
    "@types/vscode": "^1.73.0",
    "@types/vscode-webview": "^1.57.0",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mocha": "^11.7.5",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  }
}
